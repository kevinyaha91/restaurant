<% if current_user.business_owner == true %>

<h1>Add a wait time schedule to restaurant</h1>

<%= form_for @waittime do |f| %>
	<%= f.label :ownerInput8to9 %> 
	<%= f.select("ownerInput8to9", WaitTime::WAIT) %>	
	<br/>

	<%= f.label :ownerInput9to10 %> 
	<%= f.select("ownerInput8to9", WaitTime::WAIT) %>	
	<br/>


	<%= f.submit "Create This Schedule" %> 
<% end %>
<% end %>